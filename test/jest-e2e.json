{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "collectCoverageFrom": ["<rootDir>/src/**/*.(t|j)s"],
  "coveragePathIgnorePatterns": ["__generated__", "\\.spec\\.(t|j)s$"],
  "coverageDirectory": "<rootDir>/test/coverage",
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  }
}
